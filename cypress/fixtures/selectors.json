  },

  "itemSelectors": {
    "logo": "header.purchase-header div",

    "purchaseStatusBar": "div.purchase-hdr",

    "signUpTitle": "header.module-header h1",

    "usernameLabel": "label[for=\"username\"]",

    "usernameVerificationInfo": "div.module-body div div span",

    "emailLabel": "div.form-field.wide:nth-child(2) label",

    "passwordLabel": "label[for=\"password\"]",

    "createButton": "input.button.cta",

